{
  "name": "zaken-frontend",
  "version": "1.0.56",
  "private": true,
  "engines": {
    "node": ">=22",
    "npm": ">=10"
  },
  "scripts": {
    "start": "PORT=2999 vite",
    "acc": "PORT=2999 env-cmd -f .env.acceptance vite",
    "build": "vite build",
    "build:acc": "env-cmd -f .env.acceptance vite build",
    "deploy:prod": "npm version patch && git push origin HEAD --follow-tags",
    "swagger:generate-schema": "TS_NODE_PROJECT='./scripts/tsconfig.json' NODE_ENV='development' ts-node scripts/src/generateSwaggerSchema.ts",
    "lint": "eslint --max-warnings 0 --cache --ext .ts,.tsx src",
    "lint:fix": "eslint --fix --ext .ts,.tsx src",
    "test": "env-cmd -f .env.development vitest run",
    "cy": "npx cypress open",
    "cy:run": "cypress run",
    "dev": "vite",
    "preview": "vite preview"
  },
  "dependencies": {
    "@amsterdam/amsterdam-react-final-form": "^0.1.140",
    "@amsterdam/asc-assets": "^0.38.0",
    "@amsterdam/asc-ui": "^0.38.0",
    "@amsterdam/wonen-ui": "^1.0.76",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/lodash.debounce": "^4.0.7",
    "@types/lodash.isempty": "^4.4.7",
    "@types/lodash.merge": "^4.6.7",
    "@types/qs": "^6.14.0",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^18.0.3",
    "@types/styled-components": "^5.1.26",
    "@vitejs/plugin-react": "^4.6.0",
    "axios": "^1.10.0",
    "dayjs": "^1.11.13",
    "env-cmd": "^10.1.0",
    "eslint-config-react-app": "^7.0.1",
    "final-form": "^4.20.10",
    "final-form-arrays": "^3.0.2",
    "immer": "^11.0.0",
    "jwt-decode": "^4.0.0",
    "lodash.debounce": "^4.0.8",
    "lodash.isempty": "^4.4.0",
    "lodash.merge": "^4.6.2",
    "qs": "^6.14.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-final-form": "^6.5.9",
    "react-final-form-arrays": "^3.1.4",
    "react-oidc-context": "^3.3.0",
    "react-router-dom": "^6.28.0",
    "react-tooltip": "^5.29.1",
    "styled-components": "^5.3.11",
    "typescript": "5.8.2",
    "vite": "^7.0.3",
    "vite-plugin-eslint": "^1.8.1",
    "vite-tsconfig-paths": "^5.1.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.0",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@types/node": "^24.0.10",
    "cypress": "^14.3.3",
    "dotenv": "^17.2.3",
    "dtsgenerator": "^3.19.2",
    "jsdom": "^26.1.0",
    "ts-node": "^10.9.1",
    "vitest": "^3.2.4"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "IE 11"
    ]
  }
}

{
  "name": "zaken-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@datapunt/asc-assets": "^0.20.0",
    "@datapunt/asc-ui": "^0.20.0",
    "@reach/router": "^1.3.4",
    "@sentry/browser": "^5.19.2",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.6",
    "@testing-library/user-event": "^12.0.11",
    "@types/jest": "^26.0.3",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^14.0.23",
    "@types/reach__router": "^1.3.5",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.0",
    "amsterdam-react-final-form": "^0.1.77",
    "amsterdam-scaffold-form": "^0.1.26",
    "axios": "^0.19.2",
    "axios-cache-adapter": "^2.5.0",
    "dotenv-flow": "^3.2.0",
    "final-form": "^4.20.1",
    "final-form-arrays": "^3.0.2",
    "immer": "^7.0.5",
    "jwt-decode": "^2.2.0",
    "qs": "^6.9.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-final-form": "^6.5.0",
    "react-final-form-arrays": "^3.1.1",
    "react-scripts": "3.4.1",
    "resize-observer-polyfill": "^1.5.1",
    "slash-sandwich": "^1.0.7",
    "styled-components": "^5.1.1",
    "typescript": "^3.9.6"
  },
  "scripts": {
    "start": "PORT=2999 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "lint": "eslint --max-warnings 0 --cache --ext .ts,.tsx src",
    "lint:fix": "npm run lint -- --fix",
    "generate:swagger-schema": "TS_NODE_PROJECT='./scripts/tsconfig.json' NODE_ENV='development' ts-node scripts/src/generateSwaggerSchema.ts",
    "generate:form-scaffolds": "TS_NODE_PROJECT='./scripts/tsconfig.json' NODE_ENV='development' ts-node scripts/src/generateFormScaffolds.ts",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "build-storybook -s public",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "IE 11"
    ]
  },
  "devDependencies": {
    "@dtsgenerator/replace-namespace": "^1.3.1",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^5.3.19",
    "@types/dotenv-flow": "^3.0.0",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/qs": "^6.9.3",
    "cypress": "^4.9.0",
    "dtsgenerator": "^3.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-plugin-unused-imports": "^0.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "react-app-polyfill": "^1.0.6",
    "react-docgen-typescript-loader": "^3.7.2",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "**/*.ts?(x)": [
      "npm run lint:fix"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.stories.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/index.{ts,tsx}"
    ]
  }
}

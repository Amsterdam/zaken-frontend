{
  "name": "zaken-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@amsterdam/amsterdam-react-final-form": "^0.1.106",
    "@amsterdam/asc-assets": "^0.27.0",
    "@amsterdam/asc-ui": "^0.27.0",
    "@amsterdam/scaffold-form": "^0.1.35",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@reach/router": "^1.3.4",
    "@sentry/browser": "^5.29.2",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.merge": "^4.6.6",
    "@types/node": "^14.14.20",
    "@types/qs": "^6.9.5",
    "@types/reach__router": "^1.3.6",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/resize-observer-browser": "^0.1.4",
    "@types/styled-components": "^5.1.7",
    "axios": "^0.21.1",
    "env-cmd": "^10.1.0",
    "eslint-plugin-unused-imports": "^1.0.1",
    "final-form": "^4.20.1",
    "final-form-arrays": "^3.0.2",
    "immer": "^8.0.0",
    "keycloak-js": "^12.0.1",
    "lodash.debounce": "^4.0.8",
    "lodash.merge": "^4.6.2",
    "qs": "^6.9.4",
    "react": "^16.14.0",
    "react-app-polyfill": "^2.0.0",
    "react-dom": "^16.14.0",
    "react-final-form": "^6.5.2",
    "react-final-form-arrays": "^3.1.3",
    "react-scripts": "4.0.1",
    "resize-observer-polyfill": "^1.5.1",
    "slash-sandwich": "^1.0.12",
    "styled-components": "^5.2.1",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "start": "PORT=2999 react-scripts start",
    "build": "react-scripts build",
    "build:acc": "env-cmd -f .env.acceptance react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "lint": "eslint --max-warnings 0 --cache --ext .ts,.tsx src",
    "lint:fix": "eslint --fix --ext .ts,.tsx src",
    "swagger:generate-schema": "TS_NODE_PROJECT='./scripts/tsconfig.json' NODE_ENV='development' ts-node scripts/src/generateSwaggerSchema.ts",
    "form-scaffolds:generate": "TS_NODE_PROJECT='./scripts/tsconfig.json' NODE_ENV='development' ts-node scripts/src/generateFormScaffolds.ts"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "IE 11"
    ]
  },
  "devDependencies": {
    "@types/dotenv-flow": "^3.1.0",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.20",
    "dotenv-flow": "^3.2.0",
    "dtsgenerator": "^3.7.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "husky": "^4.3.7",
    "lint-staged": "^10.5.3",
    "nock": "^13.0.5",
    "react-hooks-testing-library": "^0.6.0",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "**/*.ts?(x)": [
      "npm run lint:fix"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/index.{ts,tsx}"
    ]
  }
}
